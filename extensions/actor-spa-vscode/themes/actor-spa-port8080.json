  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#64748B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#8B5CF6"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#10B981"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#F59E0B"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#00D9FF"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function", "keyword.other.special-method"],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": {
        "foreground": "#E8F4FD"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.other.inherited-class", "support.class"],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.cs"],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#8B5CF6"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css", "support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#3B82F6"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css", "support.constant.font-name.css"],
      "settings": {
        "foreground": "#10B981"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "JSON Keys",
