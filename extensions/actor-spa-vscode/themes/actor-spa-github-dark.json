  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8B949E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#F85149"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#E1E4E8"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function", "keyword.other.special-method"],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": {
        "foreground": "#E1E4E8"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.other.inherited-class", "support.class"],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.cs"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#7CE38B"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Actor-SPA Send Attribute",
      "scope": [
        "support.type.property-name.actor-spa.event",
        "entity.other.attribute-name.actor-spa.send"
      ],
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Event Types",
      "scope": ["constant.language.actor-spa.event-type"],
      "settings": {
        "foreground": "#F85149",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Payload",
      "scope": [
        "support.type.property-name.actor-spa.payload",
        "string.quoted.actor-spa.payload-value"
      ],
      "settings": {
        "foreground": "#A5C261",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA HTML Template Function",
      "scope": ["entity.name.function.tagged-template.actor-spa"],
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA CSS Template Function",
      "scope": ["entity.name.function.tagged-template.actor-spa.css"],
      "settings": {
        "foreground": "#7CE38B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "XState Keywords",
      "scope": [
        "support.function.xstate.machine-creator",
        "support.function.xstate.action",
        "support.function.xstate.guard",
        "support.type.property-name.xstate.config",
        "support.type.property-name.xstate.state-config",
        "constant.language.xstate.state-type",
        "variable.language.xstate.state",
        "support.function.xstate.state-method"
      ],
      "settings": {
        "foreground": "#FFA348",
        "fontStyle": "bold"
      }
    },
    {
      "name": "React Components",
      "scope": ["support.class.component.tsx", "support.class.component.jsx"],
      "settings": {
        "foreground": "#7CE38B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Logical Operators",
      "scope": [
        "keyword.operator.logical.and.actor-spa",
        "keyword.operator.logical.or.actor-spa",
        "keyword.operator.logical.actor-spa"
      ],
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Ternary Operators",
      "scope": [
        "keyword.operator.ternary.question.actor-spa",
        "keyword.operator.ternary.colon.actor-spa"
      ],
      "settings": {
        "foreground": "#F85149",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA State Access",
      "scope": [
        "variable.other.object.actor-spa",
        "support.function.state.matches.actor-spa",
        "support.property.state.context.actor-spa",
        "support.property.state.actor-spa"
      ],
      "settings": {
        "foreground": "#79C0FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Actor-SPA State Values",
      "scope": ["string.quoted.state-value.actor-spa"],
      "settings": {
        "foreground": "#A5C261",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Logical Expression Groups",
      "scope": [
        "punctuation.definition.group.begin.actor-spa",
        "punctuation.definition.group.end.actor-spa"
      ],
      "settings": {
        "foreground": "#D2A8FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Template Expression Sections",
      "scope": [
        "meta.logical-expression.short-circuit.actor-spa",
        "meta.logical-expression.or.actor-spa",
        "meta.conditional-expression.ternary.actor-spa",
        "meta.complex-logical-expression.actor-spa"
      ],
      "settings": {
        "foreground": "#E1E4E8"
      }
    },
    {
      "name": "Reactive-Lint TODO Comments",
      "scope": [
        "comment.block.documentation.reactive-lint-todo",
        "comment.line.double-slash.reactive-lint-todo"
      ],
      "settings": {
        "foreground": "#FFB86C",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Reactive-Lint TODO Keyword",
      "scope": ["keyword.other.todo.reactive-lint"],
      "settings": {
        "foreground": "#FF79C6",
        "fontStyle": "bold"
      }
    }
  ]
}
