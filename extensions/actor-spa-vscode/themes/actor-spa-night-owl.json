  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#637777",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#ECC48D"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.accessor",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#D6DEEB"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function", "keyword.other.special-method"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.language"],
      "settings": {
        "foreground": "#D6DEEB"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.other.inherited-class", "support.class"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type", "storage.type.cs"],
      "settings": {
        "foreground": "#FFCB8B"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#7FDBCA"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Actor-SPA Send Attribute",
      "scope": [
        "support.type.property-name.actor-spa.event",
        "entity.other.attribute-name.actor-spa.send"
      ],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Event Types",
      "scope": ["constant.language.actor-spa.event-type"],
      "settings": {
        "foreground": "#FF6363",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA Payload",
      "scope": [
        "support.type.property-name.actor-spa.payload",
        "string.quoted.actor-spa.payload-value"
      ],
      "settings": {
        "foreground": "#ECC48D",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA HTML Template Function",
      "scope": ["entity.name.function.tagged-template.actor-spa"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor-SPA CSS Template Function",
      "scope": ["entity.name.function.tagged-template.actor-spa.css"],
      "settings": {
        "foreground": "#ADDB67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "XState Keywords",
      "scope": [
        "support.function.xstate.machine-creator",
        "support.function.xstate.action",
        "support.function.xstate.guard",
        "support.type.property-name.xstate.config",
        "support.type.property-name.xstate.state-config",
        "constant.language.xstate.state-type",
        "variable.language.xstate.state",
        "support.function.xstate.state-method"
      ],
      "settings": {
        "foreground": "#F78C6C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "React Components",
      "scope": ["support.class.component.tsx", "support.class.component.jsx"],
      "settings": {
        "foreground": "#ADDB67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Reactive-Lint TODO Comments",
      "scope": [
        "comment.block.documentation.reactive-lint-todo",
        "comment.line.double-slash.reactive-lint-todo"
      ],
      "settings": {
        "foreground": "#F78C6C",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Reactive-Lint TODO Keyword",
      "scope": ["keyword.other.todo.reactive-lint"],
      "settings": {
        "foreground": "#C792EA",
        "fontStyle": "bold"
      }
    }
  ]
}
